msgid "Therion"
msgstr ""

#. [unit]: type=Footpad, id=Russle
#: Bad_Moon_Rising/scenarios/0_01_Abandoned.cfg:92
msgid "Russle"
msgstr ""

#. [unit]: type=Dwarvish Fighter, id=Altos
#: Bad_Moon_Rising/scenarios/0_01_Abandoned.cfg:105
